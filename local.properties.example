# Local configuration file (copy to local.properties and fill in values)
# This file should NOT be checked into version control

# Uncomment and set your Android SDK path (usually auto-generated by Android Studio)
# sdk.dir=/Users/your-username/Library/Android/sdk

# HMAC secret key for signing tracking requests
# REQUIRED - Build will fail without this!
# Generate with: openssl rand -base64 32
# Must match TRACKING_HMAC_SECRET on the server
TRACKING_HMAC_SECRET=your-secret-key-here

SIGNING_STORE_FILE=/Users/example/keystores/example.jks
SIGNING_STORE_PASSWORD=example password
SIGNING_KEY_ALIAS=where
SIGNING_KEY_PASSWORD=example password